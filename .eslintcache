":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"55","messages":"56","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"57","messages":"58","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"59","messages":"60","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"61","messages":"62","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\index.js",[],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\app\\app.js",[],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\item-list\\index.js",[],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\header\\index.js",[],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\random-planet\\index.js",[],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\person-details\\index.js",[],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\header\\header.js",["63","64","65","66"],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\item-list\\item-list.js",[],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\random-planet\\random-planet.js",["67"],"import React , {Component} from \"react\";\r\nimport SwapiService from \"../../services/swapi-service\";\r\nimport Spinner from \"../spinner\";\r\nimport ErrorIndicator from \"../error-indicator\";\r\n\r\nimport \"./random-planet.css\";\r\n\r\n//Удобно делать setState в отдельной ф-ции для улучщения читабельности кода (onPlanetLoaded)\r\n//удобно хранить весь объект данных в state, а потом в рендере делать его деструктуризацию\r\n//Для того,чтобы уместить спинер и остальной код компонента и не уродовать код, код компонента можно вынести приватно отдельно\r\n//Для того,чтобы вернуть несколько компонентов, не вводя искусственную div обертку - можем использовать React Fragment\r\n//JSX никак не раегирует на null, если вернуть его в render\r\n//Разделение обязанностей - есть 2 вида компонентов. Одни занимаются только отображением, другие - чисто только логикой\r\n//Если делаем импорт картинки в переменную - в ней будет лежать url, который можно пропихнуть в image \r\n\r\n\r\nexport default class RandomPlanet extends Component{\r\n\r\n    constructor(){\r\n        super();\r\n        this.swapiService = new SwapiService();\r\n        this.state = {\r\n            planet: {},\r\n            loading: true,\r\n            error: false\r\n        };\r\n        this.updatePlanet();\r\n    }\r\n\r\n    updatePlanet(){\r\n        const id = Math.floor(Math.random()*10) + 2;\r\n        //const id = 12000\r\n        this.swapiService\r\n            .getPlanet(id)\r\n            .then(this.onPlanetLoaded)\r\n            .catch(this.onError);\r\n    };\r\n\r\n    onError = (error) => {\r\n        this.setState({\r\n            error: true,\r\n            loading: false\r\n        })\r\n    };\r\n\r\n    onPlanetLoaded = (planet)=>{\r\n        this.setState({\r\n            planet, \r\n            loading: false\r\n        });\r\n    };\r\n\r\n    render(){\r\n        const {planet, loading, error} = this.state;\r\n\r\n        const hasData = !(loading || error);\r\n\r\n        const errorMessage = error ? <ErrorIndicator/> : null;\r\n        const spinner = loading ? <Spinner/>: null;\r\n        const content = hasData ? <PlanetView planet={planet}/>: null;\r\n\r\n        return(\r\n            <div className=\"random-planet jumbotron rounded\">\r\n                {errorMessage}\r\n                {spinner}\r\n                {content}\r\n            </div>\r\n        )\r\n    }\r\n};\r\n\r\nconst PlanetView = ({planet}) => {\r\n\r\n    const { id, name, population, rotationPeriod, diameter } = planet;\r\n\r\n    return (\r\n        <React.Fragment>\r\n            <img className=\"planet-image\" src={`https://starwars-visualguide.com/assets/img/planets/${id}.jpg`}/>\r\n                <div>\r\n                    <h4>{name}</h4>\r\n                    <ul className=\"list-group list-group-flush\">\r\n                        <li className=\"list-group-item\">\r\n                            <span className=\"term\">Population</span>\r\n                            <span>{population}</span>\r\n                        </li>\r\n                        <li className=\"list-group-item\">\r\n                            <span className=\"term\">Rotation period</span>\r\n                            <span>{rotationPeriod}</span>\r\n                        </li>\r\n                        <li className=\"list-group-item\">\r\n                            <span className=\"term\">Diameter</span>\r\n                            <span>{diameter}</span>\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n        </React.Fragment>\r\n    )\r\n}","C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\person-details\\person-details.js",["68"],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\services\\swapi-service.js",[],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\spinner\\index.js",[],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\spinner\\spinner.js",[],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\error-indicator\\index.js",[],"C:\\Users\\tonym\\OneDrive\\Рабочий стол\\обучение\\star-db\\training\\src\\components\\error-indicator\\error-indicator.js",[],{"ruleId":"69","severity":1,"message":"70","line":9,"column":17,"nodeType":"71","endLine":9,"endColumn":29},{"ruleId":"69","severity":1,"message":"70","line":15,"column":21,"nodeType":"71","endLine":15,"endColumn":33},{"ruleId":"69","severity":1,"message":"70","line":16,"column":21,"nodeType":"71","endLine":16,"endColumn":33},{"ruleId":"69","severity":1,"message":"70","line":17,"column":21,"nodeType":"71","endLine":17,"endColumn":33},{"ruleId":"72","severity":1,"message":"73","line":77,"column":13,"nodeType":"71","endLine":77,"endColumn":114},{"ruleId":"72","severity":1,"message":"73","line":9,"column":17,"nodeType":"71","endLine":10,"endColumn":90},"jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","JSXOpeningElement","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images."]